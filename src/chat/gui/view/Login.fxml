<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="311.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9" fx:controller="chat.gui.view.LoginController">
   <children>
      <Label layoutX="113.0" layoutY="14.0" text="Log in">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TextField fx:id="txtFieldUserName" layoutX="72.0" layoutY="89.0" promptText="Username" />
      <TextField fx:id="txtFieldPassword" layoutX="72.0" layoutY="149.0" promptText="Password" />
      <Button layoutX="113.0" layoutY="196.0" mnemonicParsing="false" onAction="#btnHandleCreateUser" text="Create user">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button layoutX="46.0" layoutY="324.0" mnemonicParsing="false" onAction="#btnHandleQuit" text="Quit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="204.0" layoutY="324.0" mnemonicParsing="false" onAction="#btnHandleLogIn" text="Log In">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
